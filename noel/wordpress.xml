<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Wordpress</Name>
  <Description>Run Wordress on Unraid.  Great for Development Environment</Description>
  <Registry>https://hub.docker.com/r/noelb888/unraid-docker-wordpress</Registry>
  <GitHub>https://github.com/noel/unraidtemplates</GitHub>
  <Repository>noelb888/unraid-docker-wordpress:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>84</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>http</Protocol>
      </Port>
    </Publish>
  </Networking>
   <Environment>
    <Variable>
      <Name>MARIADB_ROOT_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/persistent-volume/wordpressdev</HostDir>
      <ContainerDir>/bitnami</ContainerDir>
      <Mode>rw</Mode>
    </Volume>  
  </Data>
   <WebUI>http://[iP]:[PORT:84]/</WebUI>
  <Banner>http://whatsmybeeragain.com/wp-content/uploads/2013/12/Whats-My-Beer-Logo-Banner-2-01-e1318366883801.png</Banner>
  <Icon>http://fullersbarbeerfest.co.uk/files/6113/8705/6693/light-beer.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
