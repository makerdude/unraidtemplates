<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
<Category>Network:Other Other:</Category>
<Date>2018-09-12</Date>
  <Support></Support>
  <Name>Ngrok</Name>
  <Description>
    [b]Ngrok[b]
  </Description>
  <Registry>https://hub.docker.com/r/noelb888/ngrok</Registry>
  <GitHub>https://github.com/noel/unraidtemplates</GitHub>
  <Repository>noelb888/ngrok:latest</Repository>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>4040</HostPort>
        <ContainerPort>4040</ContainerPort>
        <Protocol>http</Protocol>
        <Name>http Port</Name>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>NGROK_AUTH</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>NGROK_SUBDOMAIN</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>NGROK_HOSTNAME</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>NGROK_REMOTE_ADDR</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>NGROK_USERNAME</Name>
    </Variable>
    <Variable>
      <Value>8.8.4.4</Value>
      <Name>DNS2</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>NGROK_PASSWORD</Name>
    </Variable>
    <Variable>
      <Value>http</Value>
      <Name>NGROK_PROTOCOL</Name>
    </Variable>
    <Variable>
      <Value>80</Value>
      <Name>NGROK_PORT</Name>
    </Variable>
    <Variable>
      <Value>us</Value>
      <Name>NGROK_REGION</Name>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>NGROK_BINDTLS</Name>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/ngrok/ngrok/</HostDir>
      <ContainerDir>/etc/ngrok/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <ExtraParams>--cap-add=NET_ADMIN --dns 127.0.0.1 --dns 1.1.1.1 --restart=unless-stopped</ExtraParams>
  <WebUI>http://[IP]:[PORT:4040]</WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Container>
